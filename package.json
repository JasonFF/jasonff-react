{
  "name": "jasonff-blog",
  "version": "1.0.0",
  "description": "JasonFF's blog",
  "author": "JasonFF",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "dev": "npm run webpack-dev",
    "api": "better-npm-run start-api",
    "api-dev": "better-npm-run start-api-dev",
    "server": "better-npm-run start-server",
    "build": "rm -rf dist && NODE_ENV=production webpack  --display-error-details --colors --progress -p --config webpack/prod.config.js",
    "webpack-dev": "webpack-dev-server --progress --colors --hot --inline --config webpack/dev.config.js"
  },
  "betterScripts": {
    "start-server": {
      "command": "node ./bin/app.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production",
        "PORT": "8888"
      }
    },
    "start-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "PORT": "3030"
      }
    },
    "start-api-dev": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "dev",
        "PORT": "3030"
      }
    }
  },
  "dependencies": {
    "antd": "^1.6.3",
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^3.1.2",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "node-sass": "^3.8.0",
    "rc-animate": "^2.3.0",
    "rc-queue-anim": "^0.11.11",
    "react": "^0.14.7",
    "react-dom": "^0.14.6",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.0.6",
    "redux-thunk": "^1.0.3",
    "scroll-behavior": "^0.7.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.0",
    "webpack": "^1.12.12",
    "webpack-isomorphic-tools": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-antd": "^0.4.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "install": "^0.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.3.0",
    "node-sass": "^3.6.0",
    "npm": "^3.10.3",
    "react-dom": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.2.10",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
